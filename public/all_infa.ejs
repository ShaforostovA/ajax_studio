<!doctype html>
<html lang="ru">
<head>
    <%- include('component/all_infa/head'); %>
</head>
<body>
    <div class="container">
        <%- include('navbar'); %>
    </div>
    <div class="container">
        <% information.forEach( iter => { %>
        <div class="row my-3 mx-5">
                <div class="card">
                    <div class="card-header">
                        <h5>
                            <%= iter.name + " " + iter.lastname %>
                        </h5>
                    </div>
                    <div class="card-body">
                        <blockquote class="blockquote mb-0">
                            <p>
                                Возраст: <%= iter.city %>
                            </p>
                            <p>
                                Город: <%= iter.city %>
                            </p>
                            <p>
                                Работа: <%= iter.work %>
                            </p>
                        </blockquote>
                    </div>
                </div>
        </div>
        <% } ) %>
    </div>
</body>
</html>